@using IDB.Presentation.MVC4.Models.Inputs

@model CheckboxModel

@if (Model != null)
{
    @ControlTemplates.InputCheckbox(Model.IsChecked, Model.Name, id: Model.Id, label: Model.Label,
                                    forceValidation: Model.ForceValidation, disabled: Model.Disabled,
                                    htmlClass: Model.HtmlClass, dataAttributes: Model.DataAttributes,
                                    jsonModel: Model.JsonModel)
}