@using IDB.MW.Domain.Models.Pagination;
@using IDB.MW.Domain.Models.Global;
@model DataSourceResult<MyNotificationsViewModel>

<div class="operationsHeigth">
    <ul>
        @foreach (var notification in Model.Data)
        {
            <li>
                <a href="@notification.OperationUrl">@notification.Body</a>
            </li>
        }
    </ul>
</div>
<div class="text-right">
    <a href="@Url.Action("Index", "Notifications", new {area = "Global"})">See All</a>
</div>