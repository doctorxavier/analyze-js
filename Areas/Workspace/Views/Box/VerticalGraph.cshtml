<div class='graph-content vertical-@ViewBag.Id'></div>
<script type="text/javascript">
    dataGraph["vertical-@ViewBag.Id"] = prepareData($('[box-id="@ViewBag.Id"]').attr('box-sql'));
    if (dataGraph["vertical-@ViewBag.Id"].value != 0) {
        dataGraphExcel["dash-@ViewBag.Id"] = null;
        loadVertical("vertical-@ViewBag.Id", $('[box-id="@ViewBag.Id"]').attr('box-deep'));
        $('[box-id="@ViewBag.Id"]').find('.action').text('loadVertical("vertical-@ViewBag.Id", ' + $('[box-id="@ViewBag.Id"]').attr('box-deep') + ');');
    } else {
        $('.' + "vertical-@ViewBag.Id").html("<h2>@Localization.GetText("Workspace.View.NoDataAvailable")</h2>");
        $('.' + "vertical-@ViewBag.Id").parent().find('.buttonExcel').remove();
    }

</script>

<div class="hide action"></div>