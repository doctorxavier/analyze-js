@using IDB.MW.Domain.Models.Pagination;
@using IDB.MW.Domain.Models.Global;
@model DataSourceResult<OperationViewModel>
<div class="operationsHeigth">
    <ul>
        @foreach (var op in Model.Data)
        {
            <li>
                <a href="@op.OperationUrl">@op.OperationNumber</a>
            </li>
        }
    </ul>
</div>
<div class="text-right">
    <a href="@Url.Action("Index", "Operations", new { area = "Global" })">See All</a>
</div>
