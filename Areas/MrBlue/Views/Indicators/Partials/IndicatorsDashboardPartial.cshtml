@using IDB.MW.Application.MrBlueModule.ViewModels.Indicators

@model IndicatorsDashboardViewModel

@if( Model != null )
{
    <input type="hidden" id="hidViewModel" value="@ViewBag.SerializedViewModel" />

    if( Model.History != null )
    {
        <div class="row mb20">
            <div class="col-md-7">
                @Html.Partial( "Partials/HistoryPartial" , Model.History )
            </div>
        </div>
    }
}