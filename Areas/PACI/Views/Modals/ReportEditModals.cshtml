@using IDB.Presentation.MVC4.Models.Modal
@using IDB.Presentation.MVC4.Areas.PACI.Helpers

@*Complete Modal*@

@Confluence.ModalWarning(HtmlTextHelper.ModalText("PACI.REPORT.WARNING.COMPLETE"), "modalReportComplete", new List<IDB.Presentation.MVC4.Models.Modal.ModalButtonsViewModel>()
{
     new ModalButtonsViewModel {
            Id = "btnComplete",
            Name ="btnComplete",
            Text = Localization.GetText("PACI.YES"), /*Localization.GetText("PACI.YES")*/
            Type = "buttonBlue",
            CloseAtEnd = true
        },
     new ModalButtonsViewModel {
            Id = "btnCompleteCancel",
            Name ="btnCompleteCancel",
            Text = Localization.GetText("PACI.NO"), /*Localization.GetText("PACI.NO")*/
            Type = "buttonBlue",
            CloseAtEnd = true
        }
}, showDefaultCancelButton: false)

@*Evaluate Modal*@

@Confluence.ModalWarning(HtmlTextHelper.ModalText("PACI.REPORT.WARNING.EVALUATE"), "modalEvaluateModule", new List<IDB.Presentation.MVC4.Models.Modal.ModalButtonsViewModel>()
{
     new ModalButtonsViewModel {
            Id = "btnEvaluate",
            Name ="btnEvaluate",
            Text = Localization.GetText("PACI.OK"), /*Localization.GetText("PACI.OK")*/
            Type = "buttonBlue",
            CloseAtEnd = true
        }
}, showDefaultCancelButton: false)