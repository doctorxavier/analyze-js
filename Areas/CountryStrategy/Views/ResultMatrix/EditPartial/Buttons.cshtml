@using IDB.MW.Application.CountryStrategyModule.ViewModels.ResultMatrix
@using IDB.Presentation.MVC4.Helpers

@model ResultMatrixViewModel
@{
    var boolVar = Model.Status == IDB.MW.Application.CountryStrategyModule.Enums.CountryStrategicStatusEnum.Draft;
}
@if (SecurityHelper.IsVisible(Model.FieldAccessList, "cs-result-matrix-edit-control"))
{
    <div data-id="headerButtons" class="float-right" data-idb-fieldname="cs-result-matrix-edit-control">
        @Confluence.ButtonLink(Localization.GetText("Common.Cancel"), name: "cancel")
        @Confluence.ButtonOrange(Localization.GetText("Common.Save"), name: "save")
    </div>
}
