@model IDB.MW.Application.OPUSModule.ViewModels.DeliverableService.DeliverableViewModel


<input type="hidden" id="hidViewModel" value="@ViewBag.SerializedViewModel" />
<input type="hidden" id="HasProposalAndIsCompleted" value="@Model.DeliverableCompletedYears.ToString()" />
<div class="row row-header">
    <div class="col-md-12 text-left">
        @Confluence.ButtonColapse("deliverablesTable", false)
    </div>
</div>

<div class="row row-body-40">
    <div class="col-md-12">
        @Html.Partial("Partials/Deliverable/DataTables/DeliverableTable",Model.DeliverableData)
    </div>
</div>

<div class="row row-body-50">
    <div class="col-md-12 text-right">
        <button data-action="downloadReport" name="xls" class="btn btn-sm exportExcel" style="width:30px;height:30px;"></button>
        <button data-action="downloadReport" name="pdf" class="btn btn-sm exportPDF" style="width:30px;height:30px;"></button>
    </div>
</div>
