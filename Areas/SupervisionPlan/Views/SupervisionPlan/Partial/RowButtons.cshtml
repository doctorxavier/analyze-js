@using IDB.MW.Domain.Models.Supervision.SupervisionPlan
@model SupervisionPlanModel

@if (ViewBag.EditMode != null && ((bool)ViewBag.EditMode))
{
    if (Model.SupervisionPlanVersions.Count > 0)
    {
        @Confluence.ButtonVersion(null, "version", disabled: true)
    }

    if (!Model.ActualVersionIsNew)
    {
        @Confluence.ButtonPdf("PDF", disabled: true)
        @Confluence.ButtonExcel("EXCELOPENXML", disabled: true)
    }
}
else
{
    if (Model.SupervisionPlanVersions.Count > 0)
    {
        @Confluence.ButtonVersion(null, "version", "priorVersion")
    }

    if (!Model.ActualVersionIsNew)
    {
        @Confluence.ButtonPdf("PDF", "DownloadDocument")
        @Confluence.ButtonExcel("EXCELOPENXML", "DownloadDocument")
    }
}