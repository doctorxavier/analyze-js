@using IDB.Presentation.MVC4.Helpers
@model IDB.MW.Application.PCRModule.ViewModels.ChecklistService.PCRValidationsViewModel

<input type="hidden" id="hidViewModel" value="@ViewBag.SerializedViewModel" />

<div class="row">
    <div class="col-md-10">
        <h4 class="headingh3">@Localization.GetText("PCR.Checklist.TabValidation.Checklist")</h4>
    </div>
    <div class="col-md-2 text-right">
        @if (ViewBag.Permission)
        {
            <span data-pagemode="read">
                @if (ViewBag.TeamRole || ViewBag.SPDRole)
                {
                    <button class="btn btn-primary pull-right" data-modal-allowclose="true" data-modal="verifyContentModal" data-action="resizeContentLocal">@Localization.GetText("PCR.FollowUpService.Verify.Title")</button>
                }
            </span>
        }
    </div>
</div>
<div class="row">
    <div class="col-md-12">
        @if (Model.PCRValidationsRowViewModelList != null)
        {
            @Html.Partial("Partials/DataTables/PCRValidationChecklist",
            Model.PCRValidationsRowViewModelList.Where(date => date.Date != null).ToList())
        }
    </div>
</div>
<br />



