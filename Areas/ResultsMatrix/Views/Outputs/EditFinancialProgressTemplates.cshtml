                     {{else}}
                     <input class="input min_input numberInput" 
                        id="OtherCosts_{{otherCostIndex}}__OtherCostYearPlans_{{othercostyearplanindex}}__OriginalPlan"
                        name="OtherCosts[{{otherCostIndex}}].OtherCostYearPlans[{{othercostyearplanindex}}].OriginalPlan" type="text" value="" data-referenceyear="{{referenceYear}}" data-v-min="-9999999999999999.99" data-v-max="9999999999999999.99">
                     {{/if}}   
                  </label>
               </td>
            </tr>
            <tr>
               <td>
                  <label class="editLabel" for="OtherCosts_{{otherCostIndex}}__OtherCostYearPlans_{{othercostyearplanindex}}__AnnualPlan">
                     {{#if isAnnualValueBlock}}
                     <input class="input min_input numberInput" 
