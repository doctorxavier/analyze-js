                     {{else}}
                     <input class="input min_input numberInput" 
                        id="OtherCosts_{{otherCostIndex}}__OtherCostYearPlans_{{othercostyearplanindex}}__AnnualPlan"
                        name="OtherCosts[{{otherCostIndex}}].OtherCostYearPlans[{{othercostyearplanindex}}].AnnualPlan" type="text" value="" data-referenceyear="{{referenceYear}}" data-v-min="-9999999999999999.99" data-v-max="9999999999999999.99">
