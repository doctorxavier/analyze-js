@using IDB.MW.Domain.Values

@model IDB.MW.Domain.Models.Architecture.ResultMatrix.Outputs.ResultsMatrixModel

@Html.Hidden("Outputs", "", new
{
    id = "TypeModule",
    data_operation = IDBContext.Current.Operation,
    data_urlsave = RMIndicatorValues.URL_RM_PHYSICAL_PROGRESS,
    data_urlreturn = Url.Action("IndexPhysicalProgress", "Outputs", new
    {
        area = "ResultsMatrix",
        OperationNumber = IDBContext.Current.Operation
    })
})

@Html.HiddenFor(x => x.EndProjectYear, new { data_val = "false" })
@Html.HiddenFor(x => x.OnPipelineDate.Year, new { data_val = "false" })
@Html.Hidden("IndexDeleteComponentUrl", "", new
{
    data_title = "Delete Component",
    data_route = Url.Action("IndexDeleteComponent", "Outputs", new { area = "ResultsMatrix" })
})
@Html.Hidden("IndexDeleteOutputUrl", "", new
{
    data_title = "Delete Output",
    data_route = Url.Action("IndexDeleteOutput", "Outputs", new { area = "ResultsMatrix" })
})
@Html.Hidden("CurrentServerYear", DateTime.Now.Year)
@Html.Hidden("IndexDeleteOutputYearColumnWarning", "", new
{
    data_title = "Delete Output year column",
    data_route = Url.Action("IndexDeleteOutputYearColumnWarning", "Outputs", new { area = "ResultsMatrix" })
})
@Html.Hidden("WarningMessageURL", Url.Action("IndexChangeMatrixWarningMessage", "Outputs", new
{
    area = "ResultsMatrix",
    formName = "outputsTarget"
}))
@Html.Hidden("IndexChangeEditPhysicalModeWarningMessage", "", new
{
    data_title = "Change view warning",
    data_route = Url.Action("IndexChangeEditPhysicalModeWarningMessage", "Outputs", new { area = "ResultsMatrix" })
})
@Html.Hidden("isAdministrator", Model.AccessedByAdministrator)

@Html.Hidden("hdnPredefinedIndicatorWarningUrl",
    Url.Action("Warning", "PredefinedIndicator", new { area = "ResultsMatrix" }))
@Html.Hidden("hdnPredefinedIndicatorUrl",
    Url.Action("Index", "PredefinedIndicator", new { area = "ResultsMatrix" }))
@Html.Hidden("hdnPredefinedIndicatorBackUrl", Request.Url.ToString())
@Html.Hidden("hdnUnlinkPredefinedIndicatorUrl",
    Url.Action("UnlinkPredefinedIndicator", "PredefinedIndicator", new { area = "ResultsMatrix" }))

@if (Model.showMilestones)
{
    @Html.Hidden("IndexDeleteMilestoneWarning", "", new
    {
        data_title = "Delete Milestone",
        data_route = Url.Action("IndexDeleteMilestoneWarning", "Outputs", new { area = "ResultsMatrix" })
    })
}
else
{
    @Html.Hidden("IndexDeleteMilestoneWarning", "", new
    {
        data_title = "Delete Disaggregation",
        data_route = Url.Action("IndexDeleteDisaggregationWarning", "Outputs", new { area = "ResultsMatrix" })
    })
}

@Html.Hidden("baseIndicadorId")
@Html.Hidden("hdnIntervalId", Model.interval.IntervalId)
@Html.Hidden("hdnCycleId", Model.interval.CycleId)
@Html.Hidden("hdnImpactsOutcomesLinkPredefinedIndicatorWarning",
    Localization.GetText("PI.Message.Information.PredefinedIndicatorUnitMeasure"))
